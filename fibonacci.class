
public class Fibonacci {
    static long fibo(int n) {
        int atual = 0;     // atual
        int anterior = 0;   // anterior

        for (int i = 1; i <= n; i++) {

            if (i == 1) {
                atual = 1;
                anterior = 0;
            } else {
                atual += anterior;
                anterior = atual - anterior;
            }
        }
        return atual;
    }

        public static void main(String[] args) {

            // teste do programa.Informe o N valor que deseja consultar
            int i = 600;

            System.out.print(Fibonacci.fibo(i) + "\t");

        }
}
